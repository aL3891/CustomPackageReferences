    </PackageReference>
    <PackageReference Include="ProjectJsonTask">
      <Version>1.0.0</Version>
    </PackageReference>
  </ItemGroup>
  <ItemGroup />
  <Import Project="$(MSBuildToolsPath)\Microsoft.CSharp.targets" />
  <Import Project="$(NuGetPackageRoot)projectjsontask\1.0.0\build\ProjectJsonTask.targets" Condition="Exists('$(NuGetPackageRoot)projectjsontask\1.0.0\build\ProjectJsonTask.targets')" />
